{
  "name": "@qawolf/ratio-ui",
  "private": "true",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:a11y": "node tests/accessibility.test.mjs",
    "test:visual": "vitest --run tests/visual-regression.test.js",
    "test:e2e": "playwright test",
    "test:ci": "vitest --run --exclude='**/visual-regression.test.js' && npm run test:a11y",
    "test:all": "npm run test && npm run test:a11y && npm run test:visual && npm run test:e2e"
  },
  "dependencies": {
    "papaparse": "^5.5.3",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.50.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "axe-core": "^4.10.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.0.0",
    "puppeteer": "^23.9.0",
    "vite": "^7.1.2",
    "vitest": "^2.1.8"
  }
}
